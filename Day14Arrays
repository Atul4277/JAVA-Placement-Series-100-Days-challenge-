package com.java100daychallege.java7daychallege;

public class Day14Arrays {
	
	public static void main(String[] args) {
		
		int a[] = new int[5];
		
		a[0] = 10;
		a[1] = 20;
		a[2] = 30;
		a[3] = 40;
		a[4] = 50;
		
		System.out.println("Enter the Arrays");
		for(int i = 1;i<a.length;i++) {
			System.out.println(a[i]);
		}
	}

}

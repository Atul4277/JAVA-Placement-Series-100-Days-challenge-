package com.java100daychallege.java7daychallege;

public class Day15MultidementionalArrays {
	
	public static void main(String[] args) {
		
		int arr[][] = {{1,2,3}, // i =0 j =0,1,2
				        {4,5,6}, //i =1 j =0,1,2
				        {9,8,7}}; //i =2, j= 0,1,2
		
		for(int i =0;i<3;i++) {
			for(int j =0;j<3;j++) {
				System.out.print(arr[i][j]+ " ");
			}
			System.out.println();
		}
	}

}
